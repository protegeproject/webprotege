<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .main {
            display: inline-block;
            width: 12px;
            cursor: pointer;
            height: 10px;
        }

        .container {
            position: absolute;
        }

        .container svg {
            position: absolute;
            top: 0;
            left: 0;
        }

    </ui:style>
    <g:HTMLPanel addStyleNames="{style.main}" title="Menu" ui:field="main">
        <g:HTMLPanel addStyleNames="{style.container}">
            <svg width="12" height="10">
                <rect x="0" y="0" width="12" height="2" style="stroke-width:0;fill:currentColor;"/>
                <rect x="0" y="4" width="12" height="2" style="stroke-width:0;fill:currentColor;"/>
                <rect x="0" y="8" width="12" height="2" style="stroke-width:0;fill:currentColor;"/>
            </svg>
        </g:HTMLPanel>

    </g:HTMLPanel>
</ui:UiBinder>