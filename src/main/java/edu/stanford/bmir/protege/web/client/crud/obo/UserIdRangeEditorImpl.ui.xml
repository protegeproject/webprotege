<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:text="urn:import:edu.stanford.bmir.protege.web.client.ui.library.text">
    <ui:style>
        .box-sizing-style {
            box-sizing: border-box;
            -moz-box-sizing: border-box;
            -webkit-box-sizing: border-box;
        }

    </ui:style>
    <g:HTMLPanel  addStyleNames="{style.box-sizing-style}">
        <table style="border-collapse: collapse; border-spacing: 0px;">
            <tr>
                <td>
                    <g:SuggestBox ui:field="userIdEditor" addStyleNames="{style.box-sizing-style}"/>
                </td>
                <td>
                    <text:PlaceholderTextBox ui:field="startEditor" visibleLength="8" placeholder="start" addStyleNames="{style.box-sizing-style}"/>
                </td>
                <td>
                    <text:PlaceholderTextBox ui:field="endEditor" visibleLength="8" placeholder="end" addStyleNames="{style.box-sizing-style}"/>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>