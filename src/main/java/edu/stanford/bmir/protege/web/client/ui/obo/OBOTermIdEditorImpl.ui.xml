<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <table width="95%">
            <col width="100px" align="right"/>
            <tr>
                <td align="right">
                    <g:Label text="Id" styleName="web-protege-form-label" addStyleNames="web-protege-form-layout-editor-input"/>
                </td>
                <td valign="top">
                    <g:TextBox ui:field="idField" enabled="false" width="100%" addStyleNames="web-protege-form-layout-editor-input"/>
                </td>
            </tr>
            <tr>
                <td valign="top">
                    <g:Label text="Name" addStyleNames="web-protege-form-label web-protege-form-layout-editor-input"/>
                </td>
                <td valign="top">
                    <g:TextBox ui:field="nameField" width="100%" addStyleNames="web-protege-form-layout-editor-input"/>
                </td>
            </tr>
            <tr>
                <td valign="top">
                    <g:Label text="Namespace" addStyleNames="web-protege-form-label web-protege-form-layout-editor-input"/>
                </td>
                <td valign="top">
                    <g:SuggestBox ui:field="namespaceField" width="100%" addStyleNames="web-protege-form-layout-editor-input"/>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>